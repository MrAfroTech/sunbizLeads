{
  "name": "florida-sunbiz-scraper",
  "version": "1.0.0",
  "description": "Multi-location operator lead generation from Florida Sunbiz (10+ locations)",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "run": "ts-node src/index.ts",
    "pipeline": "ts-node src/pipeline.ts",
    "sunbiz": "node scripts/sunbiz-scraper.cjs",
    "test": "jest",
    "dev": "vite",
    "client": "vite",
    "proxy": "node server/proxy.js",
    "setup": "node scripts/setup.js",
    "scrape:fl": "node scripts/runEstablished.js --state FL && node scripts/runNewBiz.js --state FL",
    "scrape:ga": "node scripts/runEstablished.js --state GA && node scripts/runNewBiz.js --state GA",
    "scrape:al": "node scripts/runEstablished.js --state AL && node scripts/runNewBiz.js --state AL"
  },
  "engines": {
    "node": ">=20"
  },
  "dependencies": {
    "express": "^4.21.0",
    "express-rate-limit": "^7.2.0",
    "@anthropic-ai/sdk": "^0.32.1",
    "@getbrevo/brevo": "^2.0.0",
    "@supabase/supabase-js": "^2.45.0",
    "cheerio": "^1.0.0",
    "dotenv": "^16.4.5",
    "google-auth-library": "^9.0.0",
    "google-spreadsheet": "^4.1.0",
    "googleapis": "^140.0.0",
    "nodemailer": "^6.9.16",
    "node-fetch": "^3.3.2",
    "p-limit": "^5.0.0"
  },
  "devDependencies": {
    "@vitejs/plugin-react": "^4.3.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "vite": "^5.4.0",
    "@types/node": "^22.0.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.0"
  }
}
